<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ApplicationScreenController">
   <center>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="275.0" layoutY="451.0" mnemonicParsing="false" onAction="#handleLogoutPressed" text="Logout">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Label layoutX="69.0" layoutY="161.0" text="Welcome to the Water Reporting System!">
               <font>
                  <Font name="Sitka Text Bold Italic" size="25.0" />
               </font>
            </Label>
            <Button layoutX="263.0" layoutY="413.0" mnemonicParsing="false" onAction="#handleEditProfilePressed" text="Edit Profile">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button layoutX="251.0" layoutY="334.0" mnemonicParsing="false" onAction="#handleActualReportsPressed" text="Actual Reports">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button layoutX="259.0" layoutY="375.0" mnemonicParsing="false" onAction="#handleReportDataPressed" text="Report Data">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button layoutX="220.0" layoutY="294.0" mnemonicParsing="false" onAction="#handleViewAllReports" text="View All Reports on Map">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Label layoutX="173.0" layoutY="249.0" text="Please select one of the following options">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
